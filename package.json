{
  "name": "paperback-writer",
  "displayName": "paperback-writer",
  "description": "Create Paperback ",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dondoko-susumu/react-native-card-media.git"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.paperback-writer.settings",
        "title": "%extension.paperback-writer.settings%",
        "group": "paperback-writer"
      },
      {
        "command": "extension.paperback-writer.pdf",
        "title": "%extension.paperback-writer.pdf%",
        "group": "paperback-writer"
      },
      {
        "command": "extension.paperback-writer.png",
        "title": "%extension.paperback-writer.png%",
        "group": "paperback-writer"
      },
      {
        "command": "extension.paperback-writer.jpeg",
        "title": "%extension.paperback-writer.jpeg%",
        "group": "paperback-writer"
      },
      {
        "command": "extension.paperback-writer.html",
        "title": "%extension.paperback-writer.html%",
        "group": "paperback-writer"
      },
      {
        "command": "extension.paperback-writer.all",
        "title": "%extension.paperback-writer.all%",
        "group": "paperback-writer"
      },
      {
        "command": "extension.paperback-writer.helloWorld",
        "title": "Hello World",
        "group": "paperback-writer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.pdf",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.settings",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.png",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.jpeg",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.html",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.all",
          "group": "paperback-writer"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.pdf",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.settings",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.png",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.jpeg",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.html",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.all",
          "group": "paperback-writer"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.pdf",
          "group": "paperback-writer"
        },
        {
          "when": "resourceLangId == markdown",
          "command": "extension.paperback-writer.settings",
          "group": "paperback-writer"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "PaperBack Writer",
      "properties": {
        "paperback-writer.outputTypes": {
          "type": "array",
          "default": [
            "pdf"
          ],
          "items": {
            "type": "string"
          },
          "description": "%paperback-writer.outputTypes.dsc%",
          "order": 0
        },
        "paperback-writer.autoOutput": {       
          "type": "boolean",
          "default": false,
          "description": "%paperback-writer.autoOutput.dsc%",
          "order": 1
        },
        "paperback-writer.listOfFileNamesExcludedFromAutoOutput": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "%paperback-writer.listOfFileNamesExcludedFromAutoOutput.dsc%",
          "order": 2
        },
        "paperback-writer.outputDirectory": {
          "type": "string",
          "default": "",
          "description": "%paperback-writer.outputDirectory.dsc%",
          "order": 3
        },
        "paperback-writer.outputDirectoryRelativePathFile": {
          "type": "boolean",
          "default": false,
          "description": "%paperback-writer.outputDirectoryRelativePathFile.dsc%",
          "order": 4
        },
        "paperback-writer.customCSS": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "%paperback-writer.customCSS.dsc%",
          "order": 5
        },
        "paperback-writer.customCSSRelativePathFile": {
          "type": "boolean",
          "default": false,
          "description": "%paperback-writer.customCSSRelativePathFile.dsc%",
          "order": 6
        },
        "paperback-writer.includeDefaultStyles": {
          "type": "boolean",
          "default": true,
          "description": "%paperback-writer.includeDefaultStyles.dsc%",
          "order": 7
        },
        "paperback-writer.baseFont": {
          "type": "string",
          "enum": [
            "",
            "Noto Sans : CY,JA,LA,VI",
            "Noto : CY,JA,LA,VI",
            "Roboto : CY,GR,LA,VI",
            "BIZ UDPGothic : JA,(CY,LA)",
            "BIZ UDPMincho : JA,(CY,LA)"
          ],
          "default": "",
          "description": "%paperback-writer.baseFont.dsc%",
          "order": 8
        },
        "paperback-writer.codeFont": {
          "type": "string",
          "enum": [
            "",
            "Source Code Pro : Code"
          ],
          "default": "",
          "description": "%paperback-writer.codeFont.dsc%",
          "order": 9
        },
        "paperback-writer.baseFontSize": {
          "type": "number",
          "default": 14,
          "description": "%paperback-writer.baseFontSize.dsc%",
          "order": 10
        },
        "paperback-writer.renderScale": {
          "type": "number",
          "default": 1,
          "description": "%paperback-writer.renderScale.dsc%",
          "order": 11
        },
        "paperback-writer.pdfDisplayHeaderFooter": {
          "type": "boolean",
          "default": true,
          "description": "%paperback-writer.pdfDisplayHeaderFooter.dsc%",
          "scope": "resource",
          "order": 12
        },
        "paperback-writer.pdfHeaderHtmlElementTemplate": {
          "type": "string",
          "default": "<div style=\"font-size: 9px; margin-left: 1cm;\"> <span class='title'></span></div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \">%%ISO-DATE%%</div>",
          "description": "%paperback-writer.pdfHeaderHtmlElementTemplate.dsc%",
          "scope": "resource",
          "order": 13
        },
        "paperback-writer.pdfFooterHtmlElementTemplate": {
          "type": "string",
          "default": "<div style=\"font-size: 9px; margin: 0 auto;\"> <span class='pageNumber'></span> / <span class='totalPages'></span></div>",
          "description": "%paperback-writer.pdfFooterHtmlElementTemplate.dsc%",
          "scope": "resource",
          "order": 14
        },
        "paperback-writer.pdfPrintBackground": {
          "type": "boolean",
          "default": true,
          "description": "%paperback-writer.pdfPrintBackground.dsc%",
          "scope": "resource",
          "order": 15
        },
        "paperback-writer.pdfPaperOrientation": {
          "type": "string",
          "enum": [
            "portrait",
            "landscape"
          ],
          "default": "portrait",
          "description": "%paperback-writer.pdfPaperOrientation.dsc%",
          "scope": "resource",
          "order": 16
        },
        "paperback-writer.pdfPageRanges": {
          "type": "string",
          "default": "",
          "description": "%paperback-writer.pdfPageRanges.dsc%",
          "scope": "resource",
          "order": 17
        },
        "paperback-writer.pdfPaperSizeFormat": {
          "type": "string",
          "enum": [
            "Letter",
            "Legal",
            "Tabloid",
            "Ledger",
            "A0",
            "A1",
            "A2",
            "A3",
            "A4",
            "A5",
            "A6"
          ],
          "default": "A4",
          "description": "%paperback-writer.pdfPaperSizeFormat.dsc%",
          "scope": "resource",
          "order": 18
        },
        "paperback-writer.width": {
          "type": "string",
          "default": "",
          "description": "%paperback-writer.width.dsc%",
          "scope": "resource"
        },
        "paperback-writer.height": {
          "type": "string",
          "default": "",
          "description": "%paperback-writer.height.dsc%",
          "scope": "resource"
        },
        "paperback-writer.margin.top": {
          "type": "string",
          "default": "1.5cm",
          "description": "%paperback-writer.margin.top.dsc%",
          "scope": "resource"
        },
        "paperback-writer.margin.bottom": {
          "type": "string",
          "default": "1cm",
          "description": "%paperback-writer.margin.bottom.dsc%",
          "scope": "resource"
        },
        "paperback-writer.margin.right": {
          "type": "string",
          "default": "1cm",
          "description": "%paperback-writer.margin.right.dsc%",
          "scope": "resource"
        },
        "paperback-writer.margin.left": {
          "type": "string",
          "default": "1cm",
          "description": "%paperback-writer.margin.left.dsc%",
          "scope": "resource"
        },
        "paperback-writer.quality": {
          "type": "number",
          "default": 100,
          "description": "%paperback-writer.quality.dsc%"
        },
        "paperback-writer.clip.x": {
          "type": [
            "number",
            "null"
          ],
          "default": "",
          "description": "%paperback-writer.clip.x.dsc%"
        },
        "paperback-writer.clip.y": {
          "type": [
            "number",
            "null"
          ],
          "default": "",
          "description": "%paperback-writer.clip.y.dsc%"
        },
        "paperback-writer.clip.width": {
          "type": [
            "number",
            "null"
          ],
          "default": "",
          "description": "%paperback-writer.clip.width.dsc%"
        },
        "paperback-writer.clip.height": {
          "type": [
            "number",
            "null"
          ],
          "default": "",
          "description": "%paperback-writer.clip.height.dsc%"
        },
        "paperback-writer.omitBackground": {
          "type": "boolean",
          "default": false,
          "description": "%paperback-writer.omitBackground.dsc%"
        },
        "paperback-writer.addBrOnSingleLineBreaksInMarkdown": {
          "type": "boolean",
          "default": false,
          "description": "%paperback-writer.addBrOnSingleLineBreaksInMarkdown.dsc%"
        },
        "paperback-writer.pathToAnExternalChromium": {
          "type": "string",
          "default": "",
          "description": "%paperback-writer.pathToAnExternalChromium.dsc%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/marked": "^6.0.0",
    "@types/minify": "^9.1.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "clean-css": "^5.3.3",
    "ejs": "^3.1.9",
    "highlight.js": "^11.9.0",
    "marked": "^12.0.0",
    "mkdirp": "^3.0.1",
    "polished": "^4.3.1",
    "puppeteer-core": "3.3.0",
    "rimraf": "^5.0.5"
  }
}
